<Window x:Class="usicMusic.View.LoopStation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:usicMusic"
        mc:Ignorable="d"
        Title="usicMusic" Width="1600" Height="900"
        WindowStyle="None" ResizeMode="NoResize"
        AllowsTransparency="True" Background="Transparent"
        WindowStartupLocation="CenterScreen">
    <Grid x:Name="AppGrid" DataContext="{Binding ElementName=root}">
        <Border x:Name="ApplicationBorder" Background="Transparent" Visibility="{Binding Visibility}" Grid.RowSpan="3">
            <Border BorderBrush="Transparent" CornerRadius="10" Margin="20">
                <Border.Background>
                    <ImageBrush ImageSource="/Resource/express.png"/>
                </Border.Background>
                <Border.BitmapEffect>
                    <DropShadowBitmapEffect Color="Black" Opacity="0.5" Direction="270" ShadowDepth="1" Softness="1" />
                </Border.BitmapEffect>
                <Label x:Name="expressMainLabel" Background="Transparent" Content="Express" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Height="98" VerticalAlignment="Top" Width="476" FontSize="48" Margin="0,43,0,0" Foreground="#FF632E2E"/>
            </Border>
        </Border>
        <Image x:Name="btnExitBack" Source="/Resource/Buttons/ExitButton.png" HorizontalAlignment="Left" VerticalAlignment="Top" Width="24" Height="24" Margin="36,32,0,0" Opacity="0.95" />
        <Image Name="btnExit" Source="/Resource/Buttons/ExitButton.png" HorizontalAlignment="Left" VerticalAlignment="Top" Width="24" Height="24" Margin="36,32,0,0" Opacity="0.95" MouseEnter="btnExit_MouseEnter" MouseLeave="btnExit_MouseLeave" MouseLeftButtonDown="btnExit_MouseLeftButtonDown" MouseLeftButtonUp="btnExit_MouseLeftButtonUp" />
        <Image Name="btnMinimizeBack" Source="/Resource/Buttons/MinButton.png" HorizontalAlignment="Left" VerticalAlignment="Top" Width="24" Height="24" Margin="72,32,0,0" Opacity="0.95" />
        <Image Name="btnMinimize" Source="/Resource/Buttons/MinButton.png" HorizontalAlignment="Left" VerticalAlignment="Top" Width="24" Height="24" Margin="72,32,0,0" Opacity="0.95" MouseEnter="btnMinimize_MouseEnter" MouseLeave="btnMinimize_MouseLeave" MouseLeftButtonDown="btnMinimize_MouseLeftButtonDown" MouseLeftButtonUp="btnMinimize_MouseLeftButtonUp" />
        <Image x:Name="btnC1" HorizontalAlignment="Left" Height="142" Margin="230,190,0,0" VerticalAlignment="Top" Width="140" Source="/Resource/Buttons/c1.png" MouseEnter="btnC1_MouseEnter" MouseLeave="btnC1_MouseLeave" MouseLeftButtonDown="btnC1_MouseLeftButtonDown" MouseLeftButtonUp="btnC1_MouseLeftButtonUp" TouchUp="btnC1_TouchUp" />
        <Image x:Name="btnC2" HorizontalAlignment="Left" Height="142" Margin="480,190,0,0" VerticalAlignment="Top" Width="140" Source="/Resource/Buttons/c2.png" MouseEnter="btnC2_MouseEnter" MouseLeave="btnC2_MouseLeave" MouseLeftButtonDown="btnC2_MouseLeftButtonDown" MouseLeftButtonUp="btnC2_MouseLeftButtonUp" TouchUp="btnC2_TouchUp" />
        <Image x:Name="btnC3" HorizontalAlignment="Left" Height="142" Margin="730,190,0,0" VerticalAlignment="Top" Width="140" Source="/Resource/Buttons/c3.png" MouseEnter="btnC3_MouseEnter" MouseLeave="btnC3_MouseLeave" MouseLeftButtonDown="btnC3_MouseLeftButtonDown" MouseLeftButtonUp="btnC3_MouseLeftButtonUp" TouchUp="btnC3_TouchUp" />
        <Image x:Name="btnC4" HorizontalAlignment="Left" Height="142" Margin="980,190,0,0" VerticalAlignment="Top" Width="140" Source="/Resource/Buttons/c4.png" MouseEnter="btnC4_MouseEnter" MouseLeave="btnC4_MouseLeave" MouseLeftButtonDown="btnC4_MouseLeftButtonDown" MouseLeftButtonUp="btnC4_MouseLeftButtonUp" TouchUp="btnC4_TouchUp" />
        <Image x:Name="btnC5" HorizontalAlignment="Left" Height="142" Margin="1230,190,0,0" VerticalAlignment="Top" Width="140" Source="/Resource/Buttons/c5.png" MouseEnter="btnC5_MouseEnter" MouseLeave="btnC5_MouseLeave" MouseLeftButtonDown="btnC5_MouseLeftButtonDown" MouseLeftButtonUp="btnC5_MouseLeftButtonUp" TouchUp="btnC5_TouchUp" />
        <CheckBox x:Name="loopDelayCheckBox_1" Content="반복여부" HorizontalAlignment="Left" Height="29" Margin="208,345,0,0" VerticalAlignment="Top" Width="92" Checked="loopDelayCheckBox_1_Checked" Unchecked="loopDelayCheckBox_1_Unchecked" />
        <ComboBox x:Name="loopDelaySecSelectionBox_1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Left" Margin="305,345,0,0" VerticalAlignment="Top" Width="70" />
        <CheckBox x:Name="loopDelayCheckBox_2" Content="반복여부" HorizontalAlignment="Left" Height="29" Margin="470,345,0,0" VerticalAlignment="Top" Width="92" Checked="loopDelayCheckBox_2_Checked" Unchecked="loopDelayCheckBox_2_Unchecked" />
        <ComboBox x:Name="loopDelaySecSelectionBox_2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Left" Margin="567,345,0,0" VerticalAlignment="Top" Width="70" />
        <CheckBox x:Name="loopDelayCheckBox_3" Content="반복여부" HorizontalAlignment="Left" Height="29" Margin="719,345,0,0" VerticalAlignment="Top" Width="92" Checked="loopDelayCheckBox_3_Checked" Unchecked="loopDelayCheckBox_3_Unchecked" />
        <ComboBox x:Name="loopDelaySecSelectionBox_3" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Left" Margin="816,345,0,0" VerticalAlignment="Top" Width="70" />
        <CheckBox x:Name="loopDelayCheckBox_4" Content="반복여부" HorizontalAlignment="Left" Height="29" Margin="976,345,0,0" VerticalAlignment="Top" Width="92" Checked="loopDelayCheckBox_4_Checked" Unchecked="loopDelayCheckBox_4_Unchecked" />
        <ComboBox x:Name="loopDelaySecSelectionBox_4" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Left" Margin="1073,345,0,0" VerticalAlignment="Top" Width="70" />
        <CheckBox x:Name="loopDelayCheckBox_5" Content="반복여부" HorizontalAlignment="Left" Height="29" Margin="1223,345,0,0" VerticalAlignment="Top" Width="92" Checked="loopDelayCheckBox_5_Checked" Unchecked="loopDelayCheckBox_5_Unchecked" />
        <ComboBox x:Name="loopDelaySecSelectionBox_5" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Left" Margin="1320,345,0,0" VerticalAlignment="Top" Width="70" />
        <Slider HorizontalAlignment="Left" Margin="400,420,0,0" VerticalAlignment="Top" Height="36" Width="1050" SmallChange="1" Maximum="60" />
        <Button x:Name="startAndStopButton" Content="START" HorizontalAlignment="Left" Height="40" Margin="160,410,0,0" VerticalAlignment="Top" Width="200" Click="Button_Click" FontSize="18" BorderThickness="0" />
        <Button x:Name="bar1" HorizontalAlignment="Left" Height="50" VerticalAlignment="Top" Width="1150" Click="bar1_Click" Margin="285,552,0,0" Background="#FF636363" BorderThickness="0" Foreground="White" />
        <Button x:Name="bar2" HorizontalAlignment="Left" Height="50" VerticalAlignment="Top" Width="1150" Click="bar1_Click" Margin="285,608,0,0" Background="#FF636363" BorderThickness="0" Foreground="White" />
        <Button x:Name="bar3" HorizontalAlignment="Left" Height="50" VerticalAlignment="Top" Width="1150" Click="bar1_Click" Margin="285,664,0,0" Background="#FF636363" BorderThickness="0" Foreground="White" />
        <Button x:Name="bar4" HorizontalAlignment="Left" Height="50" VerticalAlignment="Top" Width="1150" Click="bar1_Click" Margin="285,720,0,0" Background="#FF636363" BorderThickness="0" Foreground="White" />
        <Button x:Name="bar5" HorizontalAlignment="Left" Height="50" VerticalAlignment="Top" Width="1150" Click="bar1_Click" Margin="285,775,0,0" Background="#FF636363" BorderThickness="0" Foreground="White" />
    </Grid>
</Window>